{
  "name": "minimal-pm-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=develop nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "cross-env NODE_ENV=production babel src -s -D -d dist --presets es2015,stage-0 --require babel-polyfill",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nakh2010/minimal-pm-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nakh2010/minimal-pm-api/issues"
  },
  "homepage": "https://github.com/nakh2010/minimal-pm-api#readme",
  "dependencies": {
    "@hapi/joi": "^16.1.4",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mongoose": "^5.7.13",
    "morgan": "^1.9.1",
    "redis": "^2.8.0",
    "resource-router-middleware": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^6.5.0",
    "nodemon": "^1.19.3"
  }
}
